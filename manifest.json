{
  "manifest_version": 2,
  "name": "ezproxy url modifier",
  "version": "0.0.1",
  "description": "Click open any URL with ezproxy.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
